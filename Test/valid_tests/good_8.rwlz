// TEST: Operadores de asignaci√≥n compuesta (+=, -=, *=, /=, %)
[BepInPlugin("TestMod", "Compound Operators Test", "1.0")]

/* Test con operadores compuestos */

int UpdateStats(int health, int mana) {
    health += 25;
    mana -= 10;
    health *= 2;
    mana /= 3;
    
    int result = health % mana;
    result++;
    mana--;
    
    return result;
}